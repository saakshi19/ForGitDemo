import java.io.*;

public class ChemicalElement {
	int atomicNumber;
	String symbolicName;
	String fullName;
	public boolean isAlkaliMetal() {
		switch(atomicNumber) {
		case 3:
		case 11:
		case 19:
		case 37:
		case 55:
		case 87: return true;
		default: return false;
		}
	}
	public boolean isTransitionMetal() {
		int i;
		//21 to 31, 39 to 48, 72 to 80, and 104 to 112
		for(i=21;i<=31;i++)
		{
			if(atomicNumber==i)
				return true;
		}
		for(i=39;i<=48;i++)
		{
			if(atomicNumber==i)
			{
				return true;
			}
		}
		for(i=72;i<=80;i++) {
			if(atomicNumber==i) {
				return true;
			}
		}
		for(i=104;i<=112;i++) {
			if(atomicNumber==i) {
				return true;
			}
		}
	}
	public{
		static boolean isMetal() {
	}
		
	}
	
}
public class ChemicalElementApp{
	public static void main(String[] args) {
		
	}
}
